{
    "version": "0.1.0",
    "id": "points-att-polygon",
    "title": {
        "en": "points-att-polygon"
    },
    "description": {
        "en": "data points merged with polygon attributes based on data point location"
    },
    "jobControlOptions": ["sync-execute", "async-execute"],
    "keywords": ["astra"],
    "links": [{
        "type": "text/html",
        "rel": "about",
        "title": "GitHub repo",
        "href": "https://github.com/AstraLabuce/aquainfra-usecase-Daugava",
        "hreflang": "en-US"
    }],
    "inputs": {
        "regions": {
            "title": "regions",
            "description": "Shapefile. TODO must ask astra what this is",
            "schema": {
                "type": "string"
            },
            "minOccurs": 1,
            "maxOccurs": 1,
            "keywords": ["shapefile"]
        },
        "points": {
            "title": "points",
            "description": "Excel file. TODO must ask astra what this is",
            "schema": {
                "type": "string"
            },
            "minOccurs": 1,
            "maxOccurs": 1,
            "keywords": ["table", "excel file"]
        },
        "long_col_name": {
            "title": "Column name for longitude",
            "description": "Name of the column that contains longitude values.",
            "schema": {
                "type": "string"
            },
            "minOccurs": 0,
            "maxOccurs": 1,
            "keywords": ["longitude", "column"]
        },
        "lat_col_name": {
            "title": "Column name for latiitude",
            "description": "Name of the column that contains latitude values.",
            "schema": {
                "type": "string"
            },
            "minOccurs": 0,
            "maxOccurs": 1,
            "keywords": ["latitude", "column"]
        }
    },
    "outputs": {
        "points_att_polygon": {
            "title": "point_att_polygon result table",
            "description": "TODO must ask astra what this is",
            "schema": {
                "type": "object",
                "contentMediaType": "application/csv"
            }
        }
    },
    "example": {
        "inputs": {
            "regions": "https://www.example.xyz",
            "points": "https://www.example.xyz",
            "long_col_name": "long",
            "lat_col_name": "lat"
        }
    }
}
